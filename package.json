{
  "name": "easydx",
  "productName": "Easy DX",
  "version": "1.2.1",
  "description": "Easy DX - UI for Salesforce DX",
  "private": true,
  "bin": {
    "easydx": "index.js"
  },
  "build": {
    "appId": "com.salesforce.easydx",
    "files": [
      "src/electron.js",
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "scripts": {
    "build": "webpack --mode production && webpack --mode production --config webpack.server.config.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map",
    "server": "nodemon src/server/index.js",
    "webpackServer": "webpack-dev-server --mode development --devtool inline-source-map --config webpack.server.config.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "electron-dev": "concurrently \"BROWSER=none npm run client\" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "rm -rf dist && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lance-shi/EasyDx.git"
  },
  "author": "Lance Shi",
  "license": "GPLv3",
  "main": "src/electron.js",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "electron-is-dev": "^1.1.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "jsonfile": "^4.0.0",
    "ncp": "^2.0.0",
    "node-cmd": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "copy-webpack-plugin": "^5.0.4",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.1",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
